{
  "extends": [
    "plugin:@wordpress/eslint-plugin/recommended"
  ],
  "globals": {
    "beforeEach": "readonly",
    "describe": "readonly",
    "expect": "readonly",
    "it": "readonly",
    "jest": "readonly",
    "test": "readonly"
  },
  "settings": {
    "jsdoc": {
      "mode": "typescript"
    },
    "import/resolver": {
      "node": {
        "extensions": [".js", ".jsx", ".ts", ".tsx"]
      }
    }
  },
  "rules": {
    "jsdoc/require-param": "off",
    "jsdoc/require-param-type": "off",
    "jsdoc/require-returns-type": "off",
    "react/jsx-no-target-blank": "off"
  }
}
